// SERVERS
//

{% set data = salt['mc_bind.settings']() %}
{% for server, sdata in data['servers'].items() %}
{% if sdata['keys'] %}
server {{server}} {
  keys {
{% for k in sdata['keys'] %}
    {{k}};
{% endfor %}
  };
};
{% endif %}
{% endfor %}

